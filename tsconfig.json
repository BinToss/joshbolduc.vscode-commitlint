{
  "compilerOptions": {
    "module": "ES2022",
    "moduleResolution": "bundler",
    "target": "ES2023",
    "outDir": "out",
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "importsNotUsedAsValues": "error",
    "noUncheckedIndexedAccess": true
  },
  "exclude": ["node_modules"]
}
